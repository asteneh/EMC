<script>
    $('.attributdata').click(function () {
        $(".boderclr").removeClass("boderclr");
        /* var value = $(this).children('.inputfield').val();*/
        var value = $(this).text();
        $("input[name*='" + value + "']").addClass("boderclr");
        $(this).closest("tr").addClass('boderclr');
        $('#deviceproperty').html(value);

    });
    $('.attributcurrvalue').click(function () {
        $(".boderclr").removeClass("boderclr");
        /* var value = $(this).siblings().children('.inputfield').val();*/
        var value = $(this).siblings().text();
        $("input[name*='" + value + "']").addClass("boderclr");
        $(this).siblings().addClass("boderclr");
        $('#deviceproperty').html(value);

    });
    $("#propertysect").change(function () {
        var value = $(this).select2('data')[0].text;
        $(".boderclr").removeClass("boderclr");
        $("input[name*='" + value + "']").addClass('boderclr');
        $("input[name*='" + value + "']").closest("tr").addClass('boderclr');

        $('#deviceproperty').html(value);
    });

</script>
<script>
    var devicecode = "";
    var prefcode = "";

    $(document).on("click", ".devicepropertysetting", function () {
        devicecode = $(this).data('types');
         prefcode =  @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(prefcode));

        $('#@nameof(Model.telbrdevicecode)').val(devicecode);
        $('#@nameof(Model.telPrefcode)').val(prefcode);

        $('#@nameof(Model.devicesettingproperty)').select2({
            placeholder: 'device',
            allowclear: true,
        ajax: {
            url: '@Url.Action("getdeviceforPROPERTY", "Devices")',
            dataType: 'json',
            data: { DEVICECODE: devicecode },
                data: function (params)
                {
                return {
                    q: params.term,
                    page: params.page,
                    other: devicecode,
                };
                },
                processResults: function (data, params)
                {
                return data;
                },
                cache: true
        },
        });
    });
</script>

<script>

    var getdevicecode = "";
    var getpassword = "";

    $(document).on("click", ".devicepropertysetting", function () {
        getdevicecode = $(this).data('types');
        TeleBirrSetting(getdevicecode);

        });


    $(document).on("click", '#btnDevicesettingImport', function () {
        var devcode = $('#devicesettingpropertyInindex').val();
       
        TeleBirrSetting(devcode);
    });


    function TeleBirrSetting(devcode) {
          $.ajax({
                type: "POST",
                dataType: 'json',
                url: '@Url.Action("getPropertycurrentvalue", "Devices")',
              data: { code: devcode},

                success: function (data) {
                      $('#@nameof(Model.tel_applicationCode)').val("");
                      $('#@nameof(Model.tel_authorizationEndpointUrl)').val("");
                      $('#@nameof(Model.tel_operatorId)').val("");
                      $('#@nameof(Model.tel_password)').val("");
                      $('#@nameof(Model.tel_securityCredential)').val("");
                      $('#@nameof(Model.tel_transactionEndpointUrl)').val("");
                      $('#@nameof(Model.tel_thirdPartyId)').val("");
                      $('#@nameof(Model.tel_receiverShortCode)').val("");
                       $('#@nameof(Model.tel_resultCallbackUrl)').val("");
                       $.each(data, function (index, item) {

                              switch (item.attribute) {
                                  case "ApplicationCode":
                                      $('#@nameof(Model.tel_applicationCode)').val(item?.currentValue);

                                      break;
                                  case "AuthorizationEndpointUrl":
                                      $('#@nameof(Model.tel_authorizationEndpointUrl)').val(item?.currentValue);
                                      break;
                                  case "OperatorId":
                                      $('#@nameof(Model.tel_operatorId)').val(item.currentValue);
                                      break;
                                  case "Password":
                                      $('#@nameof(Model.tel_password)').val(item.currentValue);

                                      break;
                                  case "SecurityCredential":
                                      $('#@nameof(Model.tel_securityCredential)').val(item.currentValue);
                                      break;
                                  case "TransactionEndpointUrl":
                                      $('#@nameof(Model.tel_transactionEndpointUrl)').val(item.currentValue);
                                      break;
                            @*case "Short Code":
                                $('#@nameof(Model.telshortcode)').val(item.currentValue);
                                break;*@
                                  case "ThirdPartyId":
                                      $('#@nameof(Model.tel_thirdPartyId)').val(item.currentValue);
                                      break;
                                  case "ReceiverShortCode":
                                      $('#@nameof(Model.tel_receiverShortCode)').val(item.currentValue);
                                      break;
                                  case "ResultCallbackUrl":
                                      $('#@nameof(Model.tel_resultCallbackUrl)').val(item.currentValue);
                                      break;
                              }



                      });

                },
                error: function (x) {
                    alert(x.responseText);

                }
            });

    }

</script>
