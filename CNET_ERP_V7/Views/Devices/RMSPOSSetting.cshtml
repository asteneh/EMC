                                $('#@nameof(Model.rmspos_Theme_Fixed)').val(item.currentValue);
                                break;
                            case "Ignore zero price article":
                                $('#@nameof(Model.rms_Ignore_zero_price_article)').val(item.currentValue);
                                break;
                            case "POS Use Only Available Article":
                                $('#@nameof(Model.rms_POSUseOnlyAvailableArticle)').val(item.currentValue);
                                break;
                            case "Enable Currency Conversion":
                                $('#@nameof(Model.rms_EnableCurrencyConversion)').val(item.currentValue);
                                break;

                                     }
                                 });


                },
                error: function (x) {
                    alert(x.responseText);

                }
            });
        }

</script>


