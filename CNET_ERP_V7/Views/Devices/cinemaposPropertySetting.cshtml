<script>
    $(function () {
        $("#filterCinemaSetting").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#cinema_Setting_Table > tbody > tr").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });
    });
</script>
<script>
